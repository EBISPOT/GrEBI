                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "mgiGeneAccessionId" },
                        { "name": "--json-inject-type", "value": "Gene" }
                    ]
                },

