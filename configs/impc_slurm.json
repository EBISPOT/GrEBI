{
    "persistent_output_dir": "/nfs/production/parkinso/spot/grebi/tmp/impc",
    "worker_output_dir": "/hps/nobackup/parkinso/spot/grebi/tmp/impc",
    "use_slurm":true,
    "slurm_max_workers": {
        "ingest": 100,
        "expand_subjects": 100,
        "extract": 100
    },
    "slurm_max_memory": {
        "ingest": "32G",
        "build_equiv_db": "64G",
        "expand_subjects": "64G",
        "merge": "16G",
        "index": "64G",
        "extract": "8G"
    },
    "slurm_max_time": {
        "ingest": "8:0:0",
        "build_equiv_db": "23:0:0",
        "expand_subjects": "23:0:0",
        "merge": "23:0:0",
        "index": "23:0:0",
        "extract": "23:0:0"
    },
    "bytes_per_merged_file": 104857600,
