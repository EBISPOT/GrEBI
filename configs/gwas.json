{
    "persistent_output_dir": "/nfs/production/parkinso/spot/grebi/tmp/gwas",
    "worker_output_dir": "/hps/nobackup/parkinso/spot/grebi/tmp/gwas",
    "use_slurm":true,
    "slurm_max_workers": {
        "ingest": 100,
        "expand_subjects": 100,
        "extract": 100
    },
    "slurm_max_memory": {
        "ingest": "32G",
        "build_equiv_db": "64G",
        "expand_subjects": "64G",
        "merge": "16G",
        "index": "64G",
        "extract": "8G"
    },
    "slurm_max_time": {
        "ingest": "8:0:0",
        "build_equiv_db": "23:0:0",
        "expand_subjects": "23:0:0",
        "merge": "23:0:0",
        "index": "23:0:0",
        "extract": "23:0:0"
    },
    "bytes_per_merged_file": 104857600,
    "datasources": [
        {
            "name": "GWAS",
            "enabled": true,
            "ingests": [
                {
                    "ingest_files": [
                        "./gwas-catalog-associations_ontology-annotated.tsv",
                        "./gwas-catalog-studies-download-alternative-v1.0.2.1.txt"
                    ],
                    "ingest_script": "./target/release/grebi_ingest_gwas",
                    "ingest_args": []
                }
            ]
        },
        {
            "name": "OLS",
            "enabled": true,
            "ingests": [
                {
                    "ingest_files": ["/nfs/production/parkinso/spot/ols4/prod/slurm_pipeline/ontologies.json.gz"],
                    "ingest_script": "./target/release/grebi_ingest_ols",
                    "ingest_args": [
                        { "name": "--ontologies", "value": "efo,mp,hp,go,ro,iao,uberon,pato" }
                    ]
                }
            ]
        },
        {
            "name": "UberGraph",
            "enabled": true,
            "ingests": [
                {
                    "ingest_files": ["./ubergraph.nq.gz"],
                    "ingest_script": "./target/release/grebi_ingest_rdf",
                    "ingest_args": [
                        { "name": "--rdf-type", "value": "rdf_quads_nq" },
                        { "name": "--rdf-graph", "value": "http://reasoner.renci.org/redundant" }
                    ]
                }
            ]
        },
        {
            "name": "MP_HP",
            "enabled": true,
            "ingests": [
                {
                    "ingest_files": [
                        "./mphp.sssom.tsv"
                    ],
                    "ingest_script": "./target/release/grebi_ingest_sssom",
                    "ingest_args": []
                }
            ]
        },
        {
            "name": "IMPC",
            "enabled": true,
            "ingests": [
                {
                    "ingest_files": ["/nfs/production/parkinso/spot/jmcl/impc-kg/mouse_gene_json/*.json.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "mgiGeneAccessionId" },
                        { "name": "--json-inject-type", "value": "Gene" }
                    ]
                },

                {
                    "ingest_files": ["/nfs/production/parkinso/spot/jmcl/impc-kg/categorical_observation_json/*.json.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "observationId" },
                        { "name": "--json-inject-type", "value": "CategoricalObservation" }
                    ]
                },
                {
                    "ingest_files": ["/nfs/production/parkinso/spot/jmcl/impc-kg/embryo_specimen_json/*.json.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "specimenId" },
                        { "name": "--json-inject-type", "value": "EmbryoSpecimen" }
                    ]
                },
                {
                    "ingest_files": ["/nfs/production/parkinso/spot/jmcl/impc-kg/gene_phenotype_association_json/*.json.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "genePhenotypeAssociationId" },
                        { "name": "--json-inject-type", "value": "GenePhenotypeAssociation" }
                    ]
                },
                {
                    "ingest_files": ["/nfs/production/parkinso/spot/jmcl/impc-kg/image_record_observation_json/*.json.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "observationId" },
                        { "name": "--json-inject-type", "value": "ImageRecordObservation" }
                    ]
                },
                {
                    "ingest_files": ["/nfs/production/parkinso/spot/jmcl/impc-kg/line_experiment_json/*.json.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "experimentId" },
