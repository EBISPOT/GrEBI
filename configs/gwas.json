                {
                    "ingest_files": ["/nfs/production/parkinso/spot/jmcl/impc-kg/procedure_json/*.json.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "procedureId" },
                        { "name": "--json-inject-type", "value": "Procedure" }
                    ]
                },
                {
                    "ingest_files": ["/nfs/production/parkinso/spot/jmcl/impc-kg/specimen_experiment_json/*.json.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "experimentId" },
                        { "name": "--json-inject-type", "value": "SpecimenExperiment" }
