{
    "worker_output_dir": "/data/grebi_tmp",
    "persistent_output_dir": "/data/grebi_tmp",
    "use_slurm":false,
    "bytes_per_merged_file": 104857600,
    "datasources": [
        {
            "name": "HGNC",
            "enabled": false,
            "ingests": [
                {
                    "ingest_files": [
                        "./00_fetch_data/hgnc/hgnc.json"
                    ],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-rename-field", "value": "hgnc_id:id" },
                        { "name": "--json-inject-type", "value": "hgnc:Gene" },
                        { "name": "--json-inject-key-prefix", "value": "hgnc:" },
                        { "name": "--json-inject-value-prefix", "value": "uniprot_ids:uniprot:" },
                        { "name": "--json-inject-value-prefix", "value": "omim_id:omim:" },
                        { "name": "--json-inject-value-prefix", "value": "pubmed_id:pmid:" }
                    ]
                }
            ]
        },
        {
            "name": "UniProt",
            "enabled": false,
            "ingests": [
                {
                    "ingest_files": ["/data/up/up.n3.gz"],
                    "ingest_script": "./target/release/grebi_ingest_rdf",
                    "ingest_args": [
                        { "name": "--rdf-type", "value": "rdf_triples_xml" }
                    ]
                }
            ]
        },
        {
            "name": "UberGraph",
            "enabled": false,
            "ingests": [
                {
                    "ingest_files": ["./ubergraph.nq.gz"],
                    "ingest_script": "./target/release/grebi_ingest_rdf",
                    "ingest_args": [
                        { "name": "--rdf-type", "value": "rdf_quads_nq" },
                        { "name": "--rdf-graph", "value": "http://reasoner.renci.org/redundant" }
                    ]
                }
            ]
        },
        {
            "name": "Reactome",
            "enabled": false,
            "ingests": [
                {
                    "ingest_files": ["./01_ingest/grebi_ingest_reactome/export/reactome.json"],
                    "ingest_script": "./target/release/grebi_ingest_reactome",
                    "ingest_args": [
                    ]
                }
            ]
        },
        {
            "name": "GWAS",
            "enabled": true,
            "ingests": [
                {
                    "ingest_files": [
                        "./gwas-catalog-associations_ontology-annotated.tsv",
                        "./gwas-catalog-studies-download-alternative-v1.0.2.1.txt"
                    ],
                    "ingest_script": "./target/release/grebi_ingest_gwas",
                    "ingest_args": []
                }
            ]
        },
        {
            "name": "MP_HP",
            "enabled": true,
            "ingests": [
                {
                    "ingest_files": [
                        "./00_fetch_data/sssom/mphp.sssom.tsv"
                    ],
                    "ingest_script": "./target/release/grebi_ingest_sssom",
                    "ingest_args": []
                }
            ]
        },
        {
            "name": "OLS",
            "enabled": true,
            "ingests": [
                {
                    "ingest_files": ["/data/ontologies/ontologies.json.gz"],
                    "ingest_script": "./target/release/grebi_ingest_ols",
                    "ingest_args": [
                        { "name": "--ontologies", "value": "efo,oba,mp,hp,ro,bfo" }
                    ]
                }
            ]
        },
        {
            "name": "MONARCH",
            "enabled": false,
            "ingests": [
                {
                    "ingest_files": ["monarch-kg_edges.jsonl.gz"],
                    "ingest_script": "./target/release/grebi_ingest_kgx"
                }
            ]
        }
    ],
    "exclude_edges": [
        "ols:iri",
        "ols:curie",
        "oboinowl:id"
    ],
    "equivalence_props": [
        "owl:equivalentClass",
        "owl:equivalentProperty",
        "owl:sameAs",
        "grebi:equivalentTo",
        "ols:iri",
        "hgnc:ensembl_gene_id",
        "obo:chebi/inchi",
        "obo:chebi/inchikey",
        "obo:chebi/smiles"
    ]
}
