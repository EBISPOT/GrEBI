{
    "worker_output_dir": "/data/grebi_tmp",
    "persistent_output_dir": "/data/grebi_tmp",
    "use_slurm":false,
    "bytes_per_merged_file": 104857600,
    "datasources": [
        {
            "name": "UniProt",
            "enabled": false,
            "ingests": [
                {
                    "ingest_files": ["/nfs/production/parkinso/spot/jmcl/up/**/*.rdf.xz"],
                    "ingest_script": "./target/release/grebi_ingest_rdf",
                    "ingest_args": [
                        { "name": "--rdf-type", "value": "rdf_triples_xml" }
                    ]
                }
            ]
        },
        {
            "name": "UberGraph",
            "enabled": false,
            "ingests": [
                {
                    "ingest_files": ["./ubergraph.nq.gz"],
                    "ingest_script": "./target/release/grebi_ingest_rdf",
                    "ingest_args": [
                        { "name": "--rdf-type", "value": "rdf_quads_nq" },
                        { "name": "--rdf-graph", "value": "http://reasoner.renci.org/redundant" }
                    ]
                }
            ]
        },
        {
            "name": "GWAS",
            "enabled": true,
            "ingests": [
                {
                    "ingest_files": [
                        "./gwas-catalog-associations_ontology-annotated.tsv",
                        "./gwas-catalog-studies-download-alternative-v1.0.2.1.txt"
                    ],
                    "ingest_script": "./target/release/grebi_ingest_gwas",
                    "ingest_args": []
                }
            ]
        },
        {
            "name": "OLS",
            "enabled": false,
            "ingests": [
                {
                    "ingest_files": ["/data/ontologies/ontologies.json.gz"],
                    "ingest_script": "./target/release/grebi_ingest_ols",
                    "ingest_args": [
                        { "name": "--ontologies", "value": "efo,mp,hp,chebi,go" }
                    ]
                }
            ]
        },
        {
            "name": "MONARCH",
            "enabled": false,
            "ingests": [
                {
                    "ingest_files": ["monarch-kg_edges.jsonl.gz"],
                    "ingest_script": "./target/release/grebi_ingest_kgx"
                }
            ]
        },
        {
            "name": "IMPC",
            "enabled": false,
            "ingests": [
                {
                    "ingest_files": ["/data/impc-kg/mouse_gene.jsonl.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "mgiGeneAccessionId" },
                        { "name": "--json-inject-type", "value": "Gene" }
                    ]
                },

                {
                    "ingest_files": ["/data/impc-kg/categorical_observation.jsonl.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "observationId" },
                        { "name": "--json-inject-type", "value": "CategoricalObservation" }
                    ]
                },
                {
                    "ingest_files": ["/data/impc-kg/embryo_specimen.jsonl.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "specimenId" },
                        { "name": "--json-inject-type", "value": "EmbryoSpecimen" }
                    ]
                },
                {
                    "ingest_files": ["/data/impc-kg/gene_phenotype_association.jsonl.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "genePhenotypeAssociationId" },
                        { "name": "--json-inject-type", "value": "GenePhenotypeAssociation" }
                    ]
                },
                {
                    "ingest_files": ["/data/impc-kg/image_record_observation.jsonl.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "observationId" },
                        { "name": "--json-inject-type", "value": "ImageRecordObservation" }
                    ]
                },
                {
                    "ingest_files": ["/data/impc-kg/line_experiment.jsonl.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "experimentId" },
                        { "name": "--json-inject-type", "value": "LineExperiment" }
                    ]
                },
                {
                    "ingest_files": ["/data/impc-kg/mouse_specimen.jsonl.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "specimenId" },
                        { "name": "--json-inject-type", "value": "MouseSpecimen" }
                    ]
                },
                {
                    "ingest_files": ["/data/impc-kg/ontological_observation.jsonl.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "observationId" },
                        { "name": "--json-inject-type", "value": "OntologicalObservation" }
                    ]
                },
                {
                    "ingest_files": ["/data/impc-kg/parameter.jsonl.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "parameterId" },
                        { "name": "--json-inject-type", "value": "Parameter" }
                    ]
                },
                {
                    "ingest_files": ["/data/impc-kg/pipeline.jsonl.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "pipelineId" },
                        { "name": "--json-inject-type", "value": "Pipeline" }
                    ]
                },
                {
                    "ingest_files": ["/data/impc-kg/procedure.jsonl.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "procedureId" },
                        { "name": "--json-inject-type", "value": "Procedure" }
                    ]
                },
                {
                    "ingest_files": ["/data/impc-kg/specimen_experiment.jsonl.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "experimentId" },
                        { "name": "--json-inject-type", "value": "SpecimenExperiment" }
                    ]
                },
                {
                    "ingest_files": ["/data/impc-kg/text_observation.jsonl.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "observationId" },
                        { "name": "--json-inject-type", "value": "TextObservation" }
                    ]
                },
                {
                    "ingest_files": ["/data/impc-kg/time_series_observation.jsonl.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "observationId" },
                        { "name": "--json-inject-type", "value": "TimeSeriesObservation" }
                    ]
                },
                {
                    "ingest_files": ["/data/impc-kg/unidimensional_observation.jsonl.gz"],
                    "ingest_script": "./target/release/grebi_ingest_json",
                    "ingest_args": [
                        { "name": "--json-subject-field", "value": "observationId" },
                        { "name": "--json-inject-type", "value": "UnidimensionalObservation" }
                    ]
                }
            ]
        }
    ]
}
