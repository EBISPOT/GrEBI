{
    "name": "MONARCH",
    "enabled": true,
    "ingests": [
        {
            "ingest_files": ["monarch-kg_edges.jsonl.gz"],
            "ingest_script": "./target/release/grebi_ingest_kgx_edges",
            "ingest_args": [
                { "name": "--kgx-inject-key-prefix", "value": "monarch:" }
            ]
        },
        {
            "ingest_files": ["monarch-kg_nodes.jsonl.gz"],
            "ingest_script": "./target/release/grebi_ingest_json",
            "ingest_args": [
                { "name": "--json-rename-field", "value": "category:grebi:type" },
                { "name": "--json-inject-key-prefix", "value": "monarch:" }
             ]
        }
    ]
}